{
  "name": "react-library-vite",
  "version": "0.0.0",
  "description": "A scaffold app for building a React component/library using Vite, ready for publication to npm",
  "author": "Tom Southall",
  "keywords": [],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/react-library-vite.umd.js",
  "module": "./dist/react-library-vite.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-library-vite.es.js",
      "require": "./dist/react-library-vite.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "prop-types": "^15.8.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.3",
    "@testing-library/react": "^12.1.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^1.0.7",
    "c8": "^7.11.0",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "jsdom": "^19.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "vite": "^2.8.0",
    "vitest": "^0.3.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
